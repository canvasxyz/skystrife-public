{
  "name": "contracts",
  "license": "MIT",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "DEBUG=mud:* pnpm run build:mud && pnpm run build:abi && pnpm run build:abi-ts",
    "build:abi": "forge build",
    "build:abi-ts": "mud abi-ts",
    "build:mud": "rimraf src/codegen && pnpm run templategen && mud tablegen && mud worldgen",
    "----- EXTERNAL -----": "---------------------------",
    "devnode": "anvil --base-fee 0 --block-time 2",
    "test": "PRIVATE_KEY=0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80 DEBUG=mud:* mud test",
    "gas-report": "PRIVATE_KEY=0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80 GAS_REPORTER_ENABLED=true mud test --forgeOptions='-vvv' | gas-report --save gas-report.json --stdin",
    "dev": "pnpm run templategen && PRIVATE_KEY=0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80 pnpm mud deploy --salt=0xb00b1e --rpc=http://127.0.0.1:8545",
    "deploy:dev": "PRIVATE_KEY=0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80 DEBUG=mud:* mud deploy",
    "----- REDSTONE -----": "---------------------------",
    "deploy:redstone": "DEBUG=mud:* mud deploy --profile=redstone",
    "----- REDSTONE HOLESKY -----": "---------------------------",
    "deploy:garnet": "DEBUG=mud:* mud deploy --profile=garnet",
    "----- INTERNAL -----": "---------------------------",
    "lint": "pnpm prettier && pnpm solhint",
    "templategen": "tsx ./ts/templates/commands/templategen.ts",
    "worldgen": "mud worldgen",
    "prettier": "prettier --write 'src/**/*.sol' && prettier --write 'test/*.sol'",
    "solhint": "solhint --config ./.solhint.json 'src/**/*.sol' --fix",
    "----- DEV -----": "---------------------------",
    "migration:new": "hygen create-migration new --timestamp $(date +%s)",
    "migration:run": "tsx ./runMigration.ts"
  },
  "dependencies": {
    "@dk1a/solidity-stringutils": "0.3.3",
    "@latticexyz/cli": "2.0.5-main-ccbb4876",
    "@latticexyz/common": "2.0.5-main-ccbb4876",
    "@latticexyz/config": "2.0.5-main-ccbb4876",
    "@latticexyz/gas-report": "2.0.5-main-ccbb4876",
    "@latticexyz/schema-type": "2.0.5-main-ccbb4876",
    "@latticexyz/store": "2.0.5-main-ccbb4876",
    "@latticexyz/world": "2.0.5-main-ccbb4876",
    "@latticexyz/world-modules": "2.0.5-main-ccbb4876",
    "@openzeppelin/contracts": "4.9.3",
    "@typechain/ethers-v5": "^10.2.1",
    "@types/node": "^18.18.3",
    "@types/prompts": "2.4.9",
    "client": "workspace:*",
    "ds-test": "github:dapphub/ds-test#c9ce3f25bde29fc5eb9901842bf02850dfd2d084",
    "forge-std": "github:foundry-rs/forge-std#b4f121555729b3afb3c5ffccb62ff4b6e2818fd3",
    "hygen": "6.2.11",
    "prettier": "^2.8.8",
    "prettier-plugin-solidity": "^1.1.3",
    "prompts": "2.4.2",
    "solhint": "^3.6.2",
    "solmate": "6.2.0",
    "tsx": "^3.13.0",
    "typechain": "^8.3.1",
    "viem": "2.7.12"
  }
}
